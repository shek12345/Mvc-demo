@{
}
<table class="table table-bordered table-hover">

    <thead>

        <tr>

            <th>Id</th>

            <th>Name</th>

            <th>Company</th>

            <th>Price</th>

        </tr>

    </thead>

    <tbody>

        @foreach (mvcdemo.Models.Product product in ViewBag.Productlist)

        {

        <tr>

            <td>@product.Id</td>

            <td>@product.Name</td>

            <td>@product.Compony</td>

            <td>@product.price</td>

        </tr>

        }

    </tbody>

</table>

